{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Larik SQL Studio",
  "version": "0.3.3",
  "identifier": "com.larik.larik-sql-studio",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Larik SQL Studio",
        "width": 1200,
        "height": 800,
        "maximized": true,
        "decorations": false,
        "backgroundColor": "#161618"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}